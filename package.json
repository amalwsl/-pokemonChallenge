{
    "name": "template",
    "version": "1.0.0",
    "scripts": {
        "dev": "webpack serve  --env PRODUCTION=${PRODUCTION}  APIURL=\"https://beta.pokeapi.co/graphql/v1beta\" ",
        "build": "webpack  --env PRODUCTION=${PRODUCTION} APIURL=${APIURL}",
        "lint": "./node_modules/.bin/eslint --ext .ts,.tsx .; exit 0",
        "check": "./node_modules/.bin/tsc --project ./; exit 0",
        "format": "./node_modules/.bin/prettier './**/*.{ts,tsx,json}' --write; exit 0",
        "prepare": "husky install",
        "formatw": "npx prettier \"./**/*.{ts,tsx,json}\" --write",
        "lintw": "npx eslint --ext .ts,.tsx .",
        "checkw": "npx tsc --project ./",
        "mocks": "mocks-server",
        "compile": "graphql-codegen",
        "watch": "graphql-codegen -w"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "npx prettier --write",
            "npx eslint --fix src/",
            "tsc-files --noEmit src/global.d.ts"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.14.8",
